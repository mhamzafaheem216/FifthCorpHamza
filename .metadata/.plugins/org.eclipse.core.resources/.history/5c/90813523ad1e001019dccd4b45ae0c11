package com.fifthcorp.Hamza.commons;

public class Response<T> {
    private int status;
    private String description;
    private T data;

    public Response(int status, String description, T data) {
        this.status = status;
        this.description = description;
        this.data = data;
    }
    
    public Response(int status, String description) {
    	this.status = status;
    	this.description = description;
    }

}
